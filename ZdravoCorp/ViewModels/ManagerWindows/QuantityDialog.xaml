<Window x:Class="ZdravoCorp.ViewModels.ManagerWindows.QuantityDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp.ViewModels.ManagerWindows"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="QuantityDialog" Height="310" Width="300">
    <Grid>
        <Label  FontSize="16" VerticalAlignment="Top" Height="210" RenderTransformOrigin="0.625,0.72" Margin="0,74,0,0">Unesite Kolicinu</Label>
        <TextBox x:Name="QuantityTextBox" MaxWidth="100" Margin="47,78,-28,0" VerticalAlignment="Top"/>

        <Button  MaxWidth="100" Height="30" Click="Add_Click">Dodaj</Button>
    </Grid>
</Window>
